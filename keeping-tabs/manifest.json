{

  "name": "Keeping Tabs",
  "version": "0.0.1",
  "manifest_version": 2,

  "description": "A simple tab manager for saving and loading tabs",
  "homepage_url": "https://github.com/chunkhang/keeping-tabs",
  "icons": {
    "48": "icons/keeping-tabs-48.png",
    "96": "icons/keeping-tabs-96.png"
  },

  "permissions": [
    "tabs",
    "storage"
  ],

  "background": {
    "scripts": [
      "background/background.js"
    ]
  },

  "browser_action": {
    "default_title": "Keeping Tabs",
    "default_icon": {
      "16": "icons/keeping-tabs-16.png",
      "32": "icons/keeping-tabs-32.png"
    }
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net/ https://use.fontawesome.com/; object-src 'self'"

}
